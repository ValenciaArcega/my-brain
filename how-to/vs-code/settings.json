{
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "breadcrumbs.enabled": false,
    "chat.editor.wordWrap": "on",
    "debug.terminal.clearBeforeReusing": true,
    "diffEditor.wordWrap": "on",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingQuotes": "always",
    "editor.bracketPairColorization.enabled": false,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.folding": true,
    "editor.fontFamily": "CascadiaCode-LightItalic",
    "editor.fontWeight": "normal",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.hover.above": false,
    "editor.hover.sticky": true,
    "editor.inlineSuggest.enabled": true,
    "editor.glyphMargin": false,
    "editor.lineDecorationsWidth": 8,
    "editor.linkedEditing": true,
    "editor.mouseWheelZoom": true,
    "editor.parameterHints.cycle": false,
    "editor.quickSuggestions": {
        "comments": "on",
        "other": "on",
        "strings": "on"
    },
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "none",
    "editor.suggest.showWords": false,
    "editor.suggestOnTriggerCharacters": false,
    "editor.suggestSelection": "first",
    "editor.wordWrap": "on",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "errorLens.borderRadius": "12px",
    "errorLens.fontFamily": "Inter",
    "errorLens.fontSize": "10",
    "errorLens.fontWeight": "400",
    "errorLens.margin": "1ch",
    "errorLens.padding": "2px 1ch",
    // "errorLens.scrollbarHackEnabled": true,
    "errorLens.messageBackgroundMode": "message",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSaveDelay": 0,
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "html.format.wrapAttributes": "force",
    "javascript.format.semicolons": "remove",
    "javascript.preferences.quoteStyle": "double",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "json.sortOnSave.enable": true,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.fontFamily": "Cascadia Code Regular",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.tabs.defaultColor": "terminal.ansiBlue",
    "terminal.integrated.tabs.defaultIcon": "remote-explorer",
    "terminal.integrated.tabs.title": "Terminal",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.stickyScroll.maxLineCount": 4,
    "editor.hover.enabled": false,
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "workbench.list.defaultFindMatchType": "contiguous",
    "workbench.list.defaultFindMode": "filter",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#00000000",
        // "editor.lineHighlightBackground": "#11111295",
        // "editor.lineHighlightBorder": "#3f3f3f93"
        // "editor.lineHighlightBackground": "#00000000",
        // "editor.lineHighlightBorder": "#00000000"
    },
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "window.zoomLevel": 1,
    "vscode-edge-devtools.webhintInstallNotification": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "package-lock.json, .gitignore, vite.config.js, tsconfig.json, .hintrc, yarn.lock, pnpm-lock.yaml, bun.lockb, .eslintrc.cjs, nuget.config, app.json, babel.config.js, astro.config.mjs, env.d.ts, .env"
    },
    "editor.insertSpaces": false,
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "git.confirmSync": false,
    "workbench.tree.enableStickyScroll": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.stickyScroll.defaultModel": "foldingProviderModel",
    "terminal.integrated.fontWeightBold": "bold",
    "javascript.preferences.jsxAttributeCompletionStyle": "braces",
    "typescript.preferences.jsxAttributeCompletionStyle": "none",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "symbols.hidesExplorerArrows": false,
    "explorer.openEditors.sortOrder": "alphabetical",
    "workbench.editor.enablePreview": false,
    "[astro]": {
        "editor.defaultFormatter": "astro-build.astro-vscode"
    },
    "editor.suggest.showReferences": false,
    "errorLens.delayMode": "debounce",
    "editor.inlayHints.enabled": "off",
    "scm.showIncomingChanges": "always",
    "scm.showOutgoingChanges": "never",
    "git.openRepositoryInParentFolders": "never",
    "editor.snippetSuggestions": "none",
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning"
    ],
    "emmet.excludeLanguages": [
        "markdown",
        "javascript"
    ],
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.highlightActiveBracketPair": false,
    "supermaven.enable": {
        "*": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.statusBar.visible": false,
    "editor.lineNumbers": "off",
    "editor.minimap.renderCharacters": false,
    "workbench.iconTheme": "symbols",
    "settingsSync.ignoredSettings": [
        "editor.fontFamily",
        "editor.fontWeight",
        "editor.fontSize"
    ],
    "workbench.editor.doubleClickTabToToggleEditorGroupSizes": "off",
    "zenMode.showTabs": "none"
}